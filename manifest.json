{
  "manifest_version": 2,

  "name": "Dashlane Plugin",
  "description": "This extension will return all inputs on the DOM with their contexts",
  "version": "0.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"]
  }],
  "permissions": ["activeTab"]
}
